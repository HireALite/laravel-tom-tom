{
  "name": "hirealite/laravel-tom-tom",
  "type": "library",
  "description": "Laravel based API For TomTom Webfleet .Connect",
  "homepage": "https://github.com/HireALite/laravel-tom-tom",
  "authors": [
    {
      "name": "Hire-A-Lite DevOps",
      "email": "dev@hire-a-lite.com"
    }
  ],
  "require": {
    "php": "~5.6|~7.0",
    "illuminate/support": "~5.1.0|~5.2.0|~5.3.0",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Hirealite\\LaravelTomTom\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hirealite\\LaravelTomTom\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "license": "GPL-3.0",
  "minimum-stability": "stable"
}
